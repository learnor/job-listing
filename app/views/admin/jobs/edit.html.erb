<h1>Edit a Job</h1>
<%= simple_form_for [:admin, @job] do |form| %>
<%= form.input :title %>
<%= form.input :description %>
<%= form.input :wage_lower_bound, :label => "Min Wage" %>
<%= form.input :wage_upper_bound, :label => "Max Wage" %>
<%= form.input :contact_email %>
<%= form.input :is_hidden %>

<%= form.submit "Submit" %>
<% end %>
