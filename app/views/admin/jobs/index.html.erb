<div class="pull-righ">
  <%= link_to("Add a Job", new_admin_job_path) %>
</div>

<table class="table table-boldered">
  <% @jobs.each do |job| %>
  <tr>
    <td><%= link_to(job.title, admin_job_path(job)) %></td>
    <td><%= job.created_at %></td>
    <td><%= link_to("Edit", edit_admin_job_path(job)) %></td>
    <td><%= link_to("Delete", admin_job_path(job), :method => :delete, :data => {:confirm => "Are you sure?"}) %></td>
  </tr>
  <% end %>
</table>
